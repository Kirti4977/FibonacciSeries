y=int(input("Number of terms?"))
fibonacci_list=[0,1]
if(y<=0):
    print("Enter a positive number")
    
elif(y==1):
    print (fibonacci_list[0])
    
elif(y==2):
    print (fibonacci_list)

else:
    for i in range (2,y):
        fib= fibonacci_list[i-1] + fibonacci_list[i-2]
        fibonacci_list.append(fib)
    print(fibonacci_list)
